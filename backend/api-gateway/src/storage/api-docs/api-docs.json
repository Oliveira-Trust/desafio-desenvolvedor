                                    "starting_value"
                                ],
                                "properties": {
                                    "starting_value": {
                                        "type": "number",
                                        "format": "float",
                                        "example": "1000"
                                    },
                                    "fee_rate": {
                                        "type": "number",
                                        "format": "float",
                                        "example": "1.5"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "OK"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/fee/{id}": {
            "get": {
                "tags": [
                    "Exchange fees"
                ],
                "summary": "Retrieve fee by id",
                "operationId": "feeShow",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Fee id",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            },
            "put": {
                "tags": [
