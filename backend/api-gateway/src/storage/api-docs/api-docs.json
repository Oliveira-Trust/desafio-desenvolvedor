                                        "format": "float",
                                        "example": "1000"
                                    },
                                    "fee_rate": {
                                        "type": "number",
                                        "format": "float",
                                        "example": "1.5"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            },
            "delete": {
                "tags": [
                    "Exchange fees"
                ],
                "summary": "Delete fee by id",
                "operationId": "feeDelete",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
